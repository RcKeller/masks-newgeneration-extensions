{
	"id": "masks-newgeneration-extensions",
	"title": "Masks: A New Generation (PbtA) Extensions",
	"description": "Additional features for the unoffical Masks: A New Generation system",
	"version": "#{VERSION}#",
	"compatibility": {
		"minimum": 13,
		"verified": 13.348,
		"maximum": 15
	},
	"authors": [
		{
			"name": "RcKeller",
			"url": "https://github.com/RcKeller"
		}
	],
	"packs": [
		{
			"name": "villains",
			"label": "Villains",
			"path": "packs/villains",
			"type": "Actor",
			"system": "pbta",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		},
		{
			"name": "champions-villains",
			"label": "Villains (Champions)",
			"path": "packs/champions-villains",
			"type": "Actor",
			"system": "pbta",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		},
		{
			"name": "adversaries",
			"label": "Villains (Adversaries)",
			"path": "packs/adversaries",
			"type": "Actor",
			"system": "pbta",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		},
		{
			"name": "ported",
			"label": "Ported",
			"path": "packs/ported",
			"type": "Actor",
			"system": "pbta",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		}
	],
	"packFolders": [
		{
			"name": "Villains",
			"packs": [
				"villains"
			],
			"folders": []
		},
		{
			"name": "Villains (Adversaries)",
			"packs": [
				"adversaries"
			],
			"folders": []
		},
		{
			"name": "Villains (Champions)",
			"packs": [
				"champions-villains"
			],
			"folders": []
		},
		{
			"name": "Ported",
			"packs": [
				"ported"
			],
			"folders": []
		}
	],
	"esmodules": ["module/masks-extensions.mjs"],
	"styles": ["css/masks-extensions.css"],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "languages/en.json"
		}
	],
	"flags": {
		"hotReload": {
			"extensions": ["css", "html", "hbs", "json"],
			"paths": ["css", "languages", "templates"]
		}
	},
	"relationships": {
		"systems": [
			{
				"id": "pbta",
				"type": "system",
				"compatibility": {
					"minimum": "1.1.16"
				}
			}
		]
	},
	"url": "#{URL}#",
	"manifest": "#{MANIFEST}#",
	"download": "#{DOWNLOAD}#",
	"license": "LICENSE",
	"readme": "README.md",
	"media": [
		{
			"type": "screenshot",
			"url": "https://raw.githubusercontent.com/RcKeller/masks-newgeneration-extensions/main/images/screenshots/character-sheet_light.webp",
			"loop": false,
			"caption": "Masks character sheet screenshot from Foundry VTT."
		},
		{
			"type": "cover",
			"url": "https://raw.githubusercontent.com/RcKeller/masks-newgeneration-extensions/main/images/cover.webp",
			"loop": false,
			"caption": "Cover Image for the Unofficial Rhapsody of Blood package."
		}
	]
