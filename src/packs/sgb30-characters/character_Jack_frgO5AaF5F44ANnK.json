  "items": [
    {
      "name": "Different, but Same",
      "type": "move",
      "img": "modules/masks-newgeneration-unofficial/images/gameicons/dunce-cap-%23ffffff-%233da7db.svg",
      "system": {
        "description": "<p data-start=\"4344\" data-end=\"5061\"><em>Equivalent: Comfort and Support</em></p><p data-start=\"4344\" data-end=\"5061\">When you take a quiet moment to connect with someone who feels \"othered\" in some way, share a vulnerability, and roll <strong data-start=\"4574\" data-end=\"4586\">+Mundane</strong>.</p>",
        "moveType": "basic",
        "rollFormula": "",
        "moveResults": {
          "failure": {
            "key": "data.moveResults.failure.value",
            "label": "Complications...",
            "value": "<p>Mark Potential and the GM makes a move.</p>"
          },
          "partial": {
            "key": "data.moveResults.partial.value",
            "label": "Partial success",
            "value": "<p>Clear a condition on the target.</p>"
          },
          "success": {
            "key": "data.moveResults.success.value",
            "label": "Success!",
            "value": "<p>You both feel seen.</p><ul><li><p><strong>Clears a condition on both</strong></p></li><li><p><strong data-start=\"4704\" data-end=\"4722\">Take Influence</strong> over each other if you don’t have it</p></li><li><p><strong>Mark potential</strong> on any heroes actively listening.</p></li></ul>"
          }
        },
        "uses": 0,
        "rollType": "mundane",
        "rollMod": 0,
        "actorType": "character",
        "choices": ""
